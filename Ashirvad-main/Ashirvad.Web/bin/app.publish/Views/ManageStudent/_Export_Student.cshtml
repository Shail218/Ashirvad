@model List<Ashirvad.Data.StudentEntity>

@{
    Layout = null;
}

<table class="table" style="border: 1px groove black; border-collapse: collapse;">
    <tr>
        <th style="border: 1px groove black; border-collapse: collapse;">
            First Name
        </th>
        <th style="border: 1px groove black; border-collapse: collapse;">
            Middle Name
        </th>
        <th style="border: 1px groove black; border-collapse: collapse;">Last Name</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Birth Date</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Admission Date</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Select Course</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Select Standard</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Select School Name</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Time of School</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Batch Time</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Result of Last Year</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Percentage / Grade</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Class Name of Last Year</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Contact No / Admission Form No</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Father's Occupation</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Mother's Occupation</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Status</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Address</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Parent's Name</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Parent's Contact No (Login ID)</th>
        <th style="border: 1px groove black; border-collapse: collapse;">Password</th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.FirstName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.MiddleName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.LastName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.birth_date
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.admission_date
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.BranchCourse.course.CourseName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.BranchClass.Class.ClassName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.SchoolInfo.SchoolName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.SchoolTime
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.BatchInfo.BatchTime
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.LastYearResult
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.Grade
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.LastYearClassName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.ContactNo
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.StudentMaint.FatherOccupation
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.StudentMaint.MotherOccupation
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.RowStatus.RowStatusId
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.Address
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.StudentMaint.ParentName
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.StudentMaint.ContactNo
    </td>
    <td style="border: 1px groove black; border-collapse: collapse;">
        @item.StudentPassword2
    </td>
</tr>
    }

</table>
