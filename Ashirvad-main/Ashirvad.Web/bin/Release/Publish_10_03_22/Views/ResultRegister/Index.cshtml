@model Ashirvad.Data.Model.MarksMaintenanceModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content">
    <div class="messagealert" id="alert_container"></div>
    <div class="row">
        <section class="col-md-12">
            <div class="box box-warning">
                <div class="box-body" style="padding:8px">
                    @{
                        Html.RenderPartial("Create", new ViewDataDictionary(Model.MarksInfo));
                    }
                </div>
            </div>
        </section>
        <section class="col-md-12">
            <div class="box box-primary">
                <div class="box-body" style="padding:8px" id="StudentDetail">
                    @{
                        Html.RenderPartial("Manage", new ViewDataDictionary(Model.MarksData));
                    }
                </div>
            </div>
        </section>
    </div>
    @section Scripts {
        @*<script src="~/ThemeData/plugins/jQuery/jquery-2.2.3.min.js"></script>*@
        <script src="~/ThemeData/plugins/datatables/jquery.dataTables.min.js"></script>
        <script src="~/ThemeData/plugins/datatables/dataTables.bootstrap.min.js"></script>
        <script type="text/javascript" src="~/scripts/app/marksregister.js"></script>
        <script type="text/javascript">
            function ConfirmationDialog() {
                if (confirm("Are you sure to continue?"))
                    return true;
                else
                    return false;
            }
        </script>
    }

</section>

