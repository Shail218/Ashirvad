@model List<Ashirvad.Data.BannerEntity>
@{
    Layout = null;
}

<table class="table" style="border: 1px groove black; border-collapse: collapse;">
    <tr>

        <th style="border: 1px groove black; border-collapse: collapse;">
            Branch Name
        </th>        
        <th style="border: 1px groove black; border-collapse: collapse;">
            Sub type
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td style="border: 1px groove black; border-collapse: collapse;">
                @item.BranchInfo.BranchName
            </td>
            <td style="border: 1px groove black; border-collapse: collapse;">@item.BannerTypeText</td>
        </tr>
    }

</table>
