@model List<Ashirvad.Data.SchoolEntity>

@{
    Layout = null;
}

<table class="table" style="border: 1px groove black; border-collapse: collapse;">
    <tr>

        <th style="border: 1px groove black; border-collapse: collapse;">
            School Name
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td style="border: 1px groove black; border-collapse: collapse;">
                @item.SchoolName
            </td>

        </tr>
    }

</table>
