@model Ashirvad.Data.BranchEntity

<div class="row">
    <div class="col-md-3">
        @Html.Label("Select Status", htmlAttributes: new { @class = "control-label" })<span style="color:red">*</span>
        @Html.DropDownList("Status", new List<SelectListItem>
                       {
                       new SelectListItem{ Text="---Select-Status---", Value = "-1" },
                        new SelectListItem{ Text="Pending", Value = "1" },
                        new SelectListItem{ Text="Reject", Value = "2" },
                        new SelectListItem{ Text="Approved", Value = "3" },
                        }, new { @class = "form-control", id = "Status" })

    </div>
</div>

<table style="width: 100%;" id="studenttbl" class="table table-striped dataTable no-footer">
    <thead>
        <tr style="background-color:#005cbf;font-style:inherit;color:aliceblue">
            <th>
                Name
            </th>
            <th>
                Standard
            </th>
            <th>
                Subject
            </th>
            <th>
                Document
            </th>
            <th>
                Document Description
            </th>
            <th>
                Edit
            </th>
            <th>
                Delete
            </th>
        </tr>
    </thead>

