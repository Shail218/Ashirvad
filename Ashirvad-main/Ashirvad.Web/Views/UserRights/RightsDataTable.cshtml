
@{
    Layout = null;
}

<table style="width: 100%;" id="choiceList" class="table table-striped dataTable no-footer">
    <thead>
        <tr style="background-color:#005cbf;font-style:inherit;color:aliceblue">

            <th>
                Page
            </th>
            <th>
                Create Status
            </th>

            <th>
                Delete Status
            </th>
            <th>
                View Status
            </th>

        </tr>
    </thead>

    <tbody>
        @for (var item1 = 1; item1 < 21; item1++)
        {
            <tr>
                <td>
                    Page @item1
                </td>
                <td>
                    True
                </td>

                <td>
                    True
                </td>
                <td>
                    True
                </td>
            </tr>
        }
    </tbody>
</table>