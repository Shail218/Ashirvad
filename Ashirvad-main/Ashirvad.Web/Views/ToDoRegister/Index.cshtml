@model Ashirvad.Data.Model.ToDoMaintenanceModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/ThemeData/dist/css/model-pop-up.css" rel="stylesheet" />
<section class="content-header">
    <h1><i class="fa fa-edit" style="margin-right: 5px;"></i>To-Do</h1>
</section>
<hr />

<section class="content">
    <div class="row">
        <section class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">To-Do Register<h3>
                </div>
                <div class="box-body">
                    @{
                        Html.RenderPartial("Manage", new ViewDataDictionary(Model.ToDoData));
                    }
                </div>
            </div>
        </section>
    </div>
    @section Scripts {
        <script src="~/scripts/app/todoregister.js"></script>
        <script src="~/ThemeData/plugins/datatables/jquery.dataTables.min.js"></script>
        <script src="~/ThemeData/plugins/datatables/dataTables.bootstrap.min.js"></script>
        <script>
            $(document).ready(function () {
                $('#studenttbl').DataTable({
                    "bPaginate": true,
                    "bLengthChange": false,
                    "bFilter": true,
                    "bInfo": true,
                    "bAutoWidth": true
                });
            });
        </script>
        <script type="text/javascript">
            function ConfirmationDialog() {
                if (confirm("Are you sure to continue?"))
                    return true;
                else
                    return false;
            }
        </script>

    }


</section>

