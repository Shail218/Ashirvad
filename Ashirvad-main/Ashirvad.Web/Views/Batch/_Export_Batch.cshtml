@model List<Ashirvad.Data.BatchEntity>
@{
    Layout = null;
}

<table class="table" style="border: 1px groove black; border-collapse: collapse;">
    <tr>

        <th style="border: 1px groove black; border-collapse: collapse;">
            Course Name
        </th>
        <th style="border: 1px groove black; border-collapse: collapse;">
            Standard
        </th>
        <th style="border: 1px groove black; border-collapse: collapse;">
            Batch Time
        </th>
        <th style="border: 1px groove black; border-collapse: collapse;">
            Monday-Friday
        </th>
        <th style="border: 1px groove black; border-collapse: collapse;">
            Saturday
        </th>
        <th style="border: 1px groove black; border-collapse: collapse;">
            Sunday
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td style="border: 1px groove black; border-collapse: collapse;">
                @item.BranchCourse.course.CourseName
            </td>
            <td style="border: 1px groove black; border-collapse: collapse;">
                @item.BranchClass.Class.ClassName
            </td>
            <td style="border: 1px groove black; border-collapse: collapse;">
                @item.BatchType
            </td>
            <td style="border: 1px groove black; border-collapse: collapse;">@item.MonFriBatchTime</td>
            <td style="border: 1px groove black; border-collapse: collapse;">@item.SatBatchTime</td>
            <td style="border: 1px groove black; border-collapse: collapse;">@item.SunBatchTime</td>
        </tr>
    }

</table>
