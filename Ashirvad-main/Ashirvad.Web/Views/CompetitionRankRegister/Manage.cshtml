@model List<Ashirvad.Data.CompetitionRankEntity>
@{

    Layout = null;
}
<table style="width: 100%;" id="competitionrankregistertable" class="table table-striped dataTable no-footer">
    <thead>
        <tr style="background-color:#005cbf;font-style:inherit;color:aliceblue">
            <th>
                Competition Date
            </th>
            <th>
                Competition Name
            </th>
            <th class="CompetitionRegisterEdit">
                Edit
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model?.Count > 0) {
        foreach(var item in Model)
                {
                    <tr>
                        <td>
                            @item.competitionInfo.CompetitionDt.ToString("dd/MM/yyyy")
                        </td>
                        <td>
                            @item.competitionInfo.CompetitionName
                        </td>
                        <td>
                            <img src="~/ThemeData/images/viewIcon.png" />
                        </td>
                    </tr>
                }
        }
    </tbody>
</table>
