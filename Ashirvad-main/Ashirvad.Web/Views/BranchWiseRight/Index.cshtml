@model Ashirvad.Data.Model.BranchWiseRightMaintenanceModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<section class="content">
    <div class="messagealert" id="alert_container"></div>
    <div class="row">
        <section class="col-md-6">
            <div class="box box-warning">
                <div class="box-header with-border">
                    <h3 class="box-title">Branch Wise Right Entry</h3>
                </div>
                <div class="box-body">
                    @{
                        Html.RenderPartial("Create", new ViewDataDictionary(Model.BranchWiseRightInfo));
                    }
                </div>
            </div>
        </section>
        <section class="col-md-6">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="col-md-6">Branch Wise Right Details</h3>

                    @{
                        Html.RenderPartial("Manage", new ViewDataDictionary(Model.BranchWiseRightData));
                    }
                </div>
            </div>
        </section>
    </div>

    @section Scripts {
        <script type="text/javascript" src="~/scripts/app/branchwiseright.js"></script>
        @*<script src="~/ThemeData/plugins/jQuery/jquery-2.2.3.min.js"></script>*@
        <script src="~/ThemeData/plugins/datatables/jquery.dataTables.min.js"></script>
        <script src="~/ThemeData/plugins/datatables/dataTables.bootstrap.min.js"></script>
        <script>
            $('#subcategorytbl').DataTable({
                "bPaginate": true,
                "bLengthChange": false,
                "bFilter": true,
                "bInfo": true,
                "bAutoWidth": true,

            });
            $('#choiceList').DataTable({
                "bPaginate": false,
                "bLengthChange": false,
                "bFilter": false,
                "bInfo": false,
                "bAutoWidth": true,
                "scrollY": '30vh',
                "scrollCollapse": true

            });
        </script>
        <script type="text/javascript">
            function ConfirmationDialog() {
                if (confirm("Are you sure to continue?"))
                    return true;
                else
                    return false;
            }
        </script>

    }

</section>

