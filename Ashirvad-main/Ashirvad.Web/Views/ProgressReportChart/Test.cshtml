@model List<Ashirvad.Data.Model.TestDataPoints>

@{
    /**/
    Layout = null;
}

<h3 style="text-align: center;margin-top:1%">Test</h3>
@if (Model != null)
{
    <div class="col-md-6">
        @foreach (var item in Model)
        {
            <div class="col-md-3">
                <div class="courses-container" style="width: 160px; margin: 10px">
                    <div class="course">
                        <div class="course-info" style="text-align: center">
                            <label Style="opacity: 0.6; letter-spacing: 0.5px; text-transform: uppercase; margin: 5px 0 0 0; font-size: 12px">@item.label</label><br />
                            <a onclick="GoToTest(@item.id)" href="#" style="color:red">@item.y</a>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
    <div class="col-md-6">
        <div id="testContainer" style="height: 200px; width: 100%;"></div>
    </div>
    <div class="col-md-3"></div>
    <div class="col-md-4">
        <div class="courses-container" style="width: 150px; margin: 10px; text-align: right">
            <div class="course">
                <div class="course-info" style="text-align: center">
                    @if (Model?.Count > 0)
                    {
                        <label Style="opacity: 0.6; letter-spacing: 0.5px; text-transform: uppercase; margin: 5px 0 0 0; font-size: 12px">Overall</label><br />
                        <a href="#" style="color:red">@Model.LastOrDefault().Days %</a>
                    }

                </div>
            </div>
        </div>
    </div>
}
