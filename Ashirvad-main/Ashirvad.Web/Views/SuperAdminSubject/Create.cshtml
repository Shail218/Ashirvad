@model Ashirvad.Data.SuperAdminSubjectEntity
<form id="fSubjectDetail">
    <div class="form-group" id="dInformation">
        <div class="col-md-12">
            @Html.Label("Subject Name", htmlAttributes: new { @class = "control-label" })<span style="color:red">*</span>
            @Html.TextBoxFor(x => x.SubjectName, new { @class = "form-control required", @alt = "Subject Name", autocomplete = "off" })
            @Html.HiddenFor(x => x.SubjectID)
            @Html.HiddenFor(x => x.oldsubject)
        </div>
        <div class="form-group">
            <div class="col-md-12" style="text-align: right;">
                <br />
                <a onclick="return SaveSubject();" class="ladda-button mb-2 mr-2 btn btn-primary" data-style="expand-left">
                    <span class="ladda-label">
                        Save
                    </span>
                    <span class="ladda-spinner"></span>
                </a>
                <a href="@Url.Action("SubjectMaintenance","SuperAdminSubject", new { @subjectID=0 })" class="ladda-button mb-2 mr-2 btn btn-danger" data-style="expand-left">
                    <span class="ladda-label">
                        Cancel
                    </span>
                    <span class="ladda-spinner"></span>
                </a>
            </div>
        </div>
    </div>
</form>
