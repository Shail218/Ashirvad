

<div class="col-md-12">
    <form id="fBranchSubjectDetail">
        <div style="margin-top:10px;">
            <div class="col-md-6">
                @Html.Label("Course Name", htmlAttributes: new { @class = "control-label" })<span style="color:red">*</span>
                <select name="CourseName" id="CourseName" class="form-control requiredDDL" alt="Course Name">
                    <option value="0">--Select Course Name--</option>
                </select>
            </div>
            <div class="col-md-6">
                @Html.Label("Class Name", htmlAttributes: new { @class = "control-label" })<span style="color:red">*</span>
                <select name="ClassName" id="ClassName" class="form-control requiredDDL" alt="Class Name">
                    <option value="0">--Select Class Name--</option>
                </select>
            </div>
        </div>
    </form>
</div>
<div class="col-md-12">
    <table style="width: 100%;" id="choiceList" class="table table-striped dataTable no-footer">
        <thead>
            <tr style="background-color:#005cbf;font-style:inherit;color:aliceblue">
                <th style="width:25%">
                    <input type="checkbox" onchange="OnSelectStatus(this,'createStatus');" Id="allcreate" /> Select All
                </th>
                <th>Subject Name</th>

            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="js-export-checkbox">
                    @Html.CheckBox("10", new { @class = "createStatus" })
                </td>
                <td>
                    <label>Maths</label>
                </td>

            </tr>
            <tr>
                <td class="js-export-checkbox">
                    @Html.CheckBox("10", new { @class = "createStatus" })
                </td>
                <td>
                    <label>Science</label>
                </td>

            </tr>
            <tr>
                <td class="js-export-checkbox">
                    @Html.CheckBox("10", new { @class = "createStatus" })
                </td>
                <td>
                    <label>English</label>
                </td>

            </tr>

        </tbody>

    </table>
</div>
<div class="form-group">
    <div class="col-md-12" style="text-align: right;">
        <br />
        <a onclick="return SaveBrachClass();" class="ladda-button mb-2 mr-2 btn btn-primary" data-style="expand-left">
            <span class="ladda-label">
                Save
            </span>
            <span class="ladda-spinner"></span>
        </a>
        <a href="@Url.Action("PackageRightMaintenance","PackageRight", new { @PackageRightID=0 })" class="ladda-button mb-2 mr-2 btn btn-danger" data-style="expand-left">
            <span class="ladda-label">
                Cancel
            </span>
            <span class="ladda-spinner"></span>
        </a>
    </div>
</div>

