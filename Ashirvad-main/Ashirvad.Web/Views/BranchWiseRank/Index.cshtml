@model Ashirvad.Data.Model.TestMaintenanceModel
@{
    ViewBag.Title = "BranchWise";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content">
    <div class="row">
        <div class="messagealert" id="alert_container"></div>
        <section class="col-md-12">
            <div class="box box-warning">
                <div class="box-header with-border">
                    <h3 class="box-title">Branchwise Entry</h3>
                </div>
                <div class="box-body">
                    @{
                        Html.RenderPartial("Create", new ViewDataDictionary(Model.TestInfo));
                    }
                </div>
            </div>
        </section>
        <section class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="col-md-6">Branchwise Rank Details</h3>
                    @{
                        Html.RenderPartial("Manage", new ViewDataDictionary(Model.TestData));
                    }
                    <div class="col-md-12">
                        <div class="col-md-12 TestCompetitionEntryCreate" style="text-align: right; margin-top: 5PX">
                            <a onclick="" class="ladda-button mb-2 mr-2 btn btn-primary" data-style="expand-left">
                                <span class="ladda-label">
                                    Submit
                                </span>
                                <span class="ladda-spinner"></span>
                            </a>
                        </div>
                    </div>
                </div>
                
            </div>
            
        </section>       
       </div>
    @section Scripts {
        <script src="~/ThemeData/plugins/datatables/jquery.dataTables.min.js"></script>
        <script src="~/ThemeData/plugins/datatables/dataTables.bootstrap.min.js"></script>
        @*<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/js/bootstrap-datepicker.js"></script>*@
        <script>
                $(document).ready(function () {

                    $('#studenttbl').DataTable({
                        "bPaginate": true,
                        "bLengthChange": false,
                        "bFilter": true,
                        "bInfo": true,
                        "bAutoWidth": true,
                        "order": [[0, "desc"]]
                    });
                });
            </script>
        <script type="text/javascript">
            function ConfirmationDialog() {
                if (confirm("Are you sure to continue?"))
                    return true;
                else
                    return false;
            }
        </script>
    }
</section>


