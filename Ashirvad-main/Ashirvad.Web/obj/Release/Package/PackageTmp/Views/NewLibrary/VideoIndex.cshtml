@model Ashirvad.Data.Model.LibraryNewMaintenanceModel

@{
    ViewBag.Title = "VideoIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<section class="content">
    <div class="messagealert" id="alert_container"></div>
    <div class="row">
        <section class="col-md-5">
            <div class="box box-warning">
                <div class="box-header with-border">
                    <h3 class="box-title">Library Video Entry</h3>
                </div>
                <div class="box-body">
                    @{ Html.RenderPartial("VideoCreate", new ViewDataDictionary(Model.LibraryInfo)); }
                </div>
            </div>
        </section>
        <section class="col-md-7">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3>Library Video Details</h3>
                    <div style="overflow-x:scroll">
                        @{ Html.RenderPartial("VideoManage", new ViewDataDictionary(Model.LibraryData)); }
                    </div>
                    
                </div>
            </div>
        </section>
    </div>

    @section Scripts {
        <script type="text/javascript" src="~/scripts/app/LibraryImage.js"></script>
        @*<script src="~/ThemeData/plugins/jQuery/jquery-2.2.3.min.js"></script>*@
        <script src="~/ThemeData/plugins/datatables/jquery.dataTables.min.js"></script>
        <script src="~/ThemeData/plugins/datatables/dataTables.bootstrap.min.js"></script>
        <script>
$(document).ready(function () {

                $('#studenttbl').DataTable({
                    "bPaginate": true,
                    "bLengthChange": false,
                    "bFilter": true,
                    "bInfo": true,
                    "bAutoWidth": true
                });


            });</script>
        <script type="text/javascript">
function ConfirmationDialog() {
                if (confirm("Are you sure to continue?"))
                    return true;
                else
                    return false;
            }</script>

    }

</section>
